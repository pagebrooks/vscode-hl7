{
    "name": "hl7",
    "displayName": "HL7",
    "sponsor": {
         "url": "https://github.com/sponsors/pagebrooks"
    },
    "description": "HL7 Language Support for Visual Studio Code",
    "version": "0.0.22",
    "publisher": "pbrooks",
    "icon": "images/hl7-icon.png",
    "galleryBanner": {
        "color": "#EAB8B4",
        "theme": "light"
    },
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/pagebrooks/vscode-hl7/issues"
    },
    "homepage": "https://github.com/pagebrooks/vscode-hl7/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/pagebrooks/vscode-hl7.git"
    },
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Languages",
        "Formatters",
        "Other"
    ],
    "keywords": [
        "hl7",
        "health level 7",
        "healthcare",
        "medical",
        "interoperability",
        "health-information-exchange"
    ],
    "activationEvents": [
        "onLanguage:hl7"
    ],
	"main": "./extension",
    "contributes": {
        "languages": [
            {
                "id": "hl7",
                "aliases": [
                    "HL7",
                    "hl7"
                ],
                "extensions": [
                    ".hl7"
                ],
                "configuration": "./hl7.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "hl7",
                "scopeName": "text.hl7",
                "path": "./syntaxes/hl7.tmLanguage.json"
            }
        ],
        "commands": [{
            "command": "extension.filterSegment",
            "title": "HL7: Filter Segment"
        },{
            "command": "extension.tokenizeLine",
            "title": "HL7: Tokenize Line"
        },{
            "command": "extension.toggleAutoTokenize",
            "title": "HL7: Toggle Auto-Tokenize"
        },{
            "command": "extension.sendMessage",
            "title": "HL7: Send Message"
        },{
            "command": "extension.startListener",
            "title": "HL7: Start MLLP Listener"
        },{
            "command": "extension.stopListener",
            "title": "HL7: Stop MLLP Listener"
        }],
        "configuration": {
            "title": "HL7",
            "properties": {
                "hl7.mllp.host": {
                    "type": "string",
                    "default": "",
                    "description": "Default MLLP host"
                },
                "hl7.mllp.port": {
                    "type": "number",
                    "default": 2575,
                    "description": "Default MLLP port"
                }
            }
        }
    },
    "scripts": {
        "test": "mocha --require test/setup.js test/**/*.test.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.75.0",
        "mocha": "^10.2.0"
    }
}